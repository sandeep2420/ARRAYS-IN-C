//WRITE A PROGRAM TO PRINT THE LEFT SIDE ELEMENTS OF MAX VALUES FROM THE GIVEN ARRAY

#include<stdio.h>
int main()
{
	int size;
	printf("Enter the size of the array \n");
	scanf("%d",&size);
	int a[size];
	int i;
	int j;
	for(i=0;i<=(size-1);i++)
	{
		printf("Enter the %d value:\n",(i+1));
		scanf("%d",&a[i]);
	}
	int max=a[0];
	int index=0;
	for (i=0; i<=size-1;i++)
	{
		if(a[1]>max)
		{
			index=i;
		}
	}
	for (i=0;i<index;i++)
	{
		printf("%d\n",a[i]);
	}
	return 0;
}
