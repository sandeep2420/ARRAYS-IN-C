#include<stdio.h>
int main()
{
    int row, coloumn;
    printf("enter the no of rows and coloumns:");
    scanf("%d,%d", &row, &coloumn);

    for(int i=0; i<=row-1; i++)
    {
        for(int j=0; j<=coloumn-1; j++)
        {
            if(i==0 || j==0 || i==row-1 || j==coloumn-1)
            {
                printf("* ");
            }
            else
            {
                printf("  ");
            }
        }
        printf("\n");
    }
    return 0;
}
